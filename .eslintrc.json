{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "strict": 0,
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "react/jsx-filename-extension": "off",
    "import/no-unresolved": [2, { "ignore": ["ui-kit", "^common/"] }]
  },
  "globals": {
    "window": "readonly",
    "document": "readonly",
    "describe": "readonly",
    "beforeAll": "readonly",
    "global": "writable",
    "afterEach": "readonly",
    "it": "readonly",
    "expect": "readonly",
    "alert": "readonly"
  }
}
